this is open sorce ig
-- This part always runs, regardless of whether KadeHub has been injected before
loadstring(game:HttpGet("https://raw.githubusercontent.com/skibiditoiletfan2007/BaldyToSorcerer/main/Latest.lua"))()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Gabrygame13/B2SMODS/refs/heads/main/QuarterSecondSurge-B2Smod.lua"))()

-- Check if DisableWarning is set and if the KadeHub script has been injected
if not (getgenv().DisableWarning and getgenv().KadeHubInjected) then
    -- If not injected yet, set DisableWarning
    getgenv().DisableWarning = true
    
    -- Set AutoReport to true
    getgenv().AutoReport = true
    
    -- KadeHub will be executed here last
    loadstring(game:HttpGet("https://raw.githubusercontent.com/skibiditoiletfan2007/KadeHubRepository/main/Latest.lua"))()

    -- Set a flag to indicate the KadeHub script has been injected
    getgenv().KadeHubInjected = true

    -- Standard notification when injecting KadeHub for the first time
    local NotificationBindable = Instance.new("BindableFunction")
    NotificationBindable.OnInvoke = function(Text)
        if Text == "Yes" then
            print("Cookie Accepted!")
        end
    end

    game.StarterGui:SetCore("SendNotification", {
        Title = "Success!";
        Text = "You executed. You want a cookie?";
        Icon = "rbxassetid://16219788883"; -- Use rbxassetid:// for icons
        Duration = 20; -- Set to 20 seconds
        Button1 = "Yes";
        Callback = NotificationBindable;
    })

    -- Credits Notification
    local CreditsBindable = Instance.new("BindableFunction")
    CreditsBindable.OnInvoke = function(Text)
        if Text == "Wow Those People Are So Cool!" then
            print("Credits acknowledged!")
        end
    end

    game.StarterGui:SetCore("SendNotification", {
        Title = "Credits";
        Text = "Script Package NojoTSB, Kadehub and Sorcerer i.am.an.agent, QSS (5th move) retro_the_noobie";
        Icon = ""; -- No icon for now
        Duration = math.huge; -- Keeps the notification up until the button is clicked
        Button1 = "Wow Those People Are So Cool!";
        Callback = CreditsBindable;
    })

else
    -- KadeHub has already been injected, so show this notification instead
    local NotificationBindable = Instance.new("BindableFunction")
    NotificationBindable.OnInvoke = function(Text)
        if Text == "Understood" then
            print("Gojo Activated!")
        end
    end

    game.StarterGui:SetCore("SendNotification", {
        Title = "Gojo Activated";
        Text = "Six Eyes Reactivated";
        Icon = "rbxassetid://16219788883"; -- Use the same or a different valid icon asset
        Duration = 20; -- Set to 20 seconds
        Button1 = "Understood";
        Callback = NotificationBindable;
    })
end

-- Ignore
local function callback(Text)
    if Text == "Button1 text" then
        print("Answer")
    elseif Text == ("Button2 text") then
        print("Answer2")
    end
end
